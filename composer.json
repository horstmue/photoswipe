{
  "name": "horstmue/photoswipe",
  "type": "typo3-cms-extension",
  "description": "TYPO3 PhotoSwipe-Plugin for native image-enlargement and caption",
  "license": "GPL-2.0-or-later",
  "version": "2.0.10",
  "require": {
    "php": "^8.1 || ^8.2 || ^8.3",
    "typo3/cms-backend": "^12.4 || ^13.2 || ^13.3 || ^13.4",
    "typo3/cms-core": "^12.4 || ^13.2 || ^13.3 || ^13.4"
  },
  "authors": [
    {
      "name": "Tobias Eichelbegrer",
      "role": "Developer"
    },
    {
      "name": "Natascha Becker",
      "role": "Developer"
    }
  ],
  "keywords": [
    "typo3",
    "extension",
    "photoswipe",
    "lightbox",
    "image zoom",
    "layer"
  ],
  "support": {
    "issues": "https://github.com/j0nnybrav079/photoswipe/issues"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "photoswipe"
    }
  },
  "replace": {
    "typo3-ter/photoswipe": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Tei\\PhotoSwipe\\": "Classes"
    }
  }
}
